<odoo>

    <record id="view_form_sales_team" model="ir.ui.view">
        <field name="name">sales.team.form</field>
        <field name="model">sales.team</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="sales_leader"/>
                        <field name="sales_persons"/>
                        <field name="total_transaksi"/>
                        <field name="total_komisi"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tree_sales_team" model="ir.ui.view">
        <field name="name">sales.team.tree</field>
        <field name="model">sales.team</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="sales_leader"/>
                <field name="sales_persons"/>
                <field name="total_transaksi"/>
                <field name="total_komisi"/>
            </tree>
        </field>
    </record>
</odoo>
